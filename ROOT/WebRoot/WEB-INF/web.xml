<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <servlet>
    <servlet-name>ExpIdInput</servlet-name>
    <servlet-class>servlet.ExpIdInput</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DDosProcess</servlet-name>
    <servlet-class>servlet.DDosProcess</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>TrojanProcess</servlet-name>
    <servlet-class>servlet.TrojanProcess</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>WormProcess</servlet-name>
    <servlet-class>servlet.WormProcess</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ExpIdInput</servlet-name>
    <url-pattern>/servlet/ExpIdInput</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DDosProcess</servlet-name>
    <url-pattern>/servlet/DDosProcess</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TrojanProcess</servlet-name>
    <url-pattern>/servlet/TrojanProcess</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>WormProcess</servlet-name>
    <url-pattern>/servlet/WormProcess</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
      <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  
</web-app>